{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/*.{css,scss,js}",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.{png,gif,ico,jpg,svg,eot,ttf,woff,woff2}",
      "headers": {
        "cache-control": "public, max-age=31536000, must-revalidate"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-store, must-revalidate, proxy-revalidate, no-cache, max-age=0"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "networking": {
    "allowedIpRanges": ["AzureFrontDoor.Backend"]
  },
  "forwardingGateway": {
    "requiredHeaders": {
      "X-Azure-FDID": "9e932fab-c1d2-40c2-bd9c-02c76194011b"
    },
    "allowedForwardedHosts": [
      "piste-f8a2azddbwbmbfd2.z01.azurefd.net",
