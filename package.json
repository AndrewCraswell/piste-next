{
  "name": "piste-next",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "yarn gql && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:ts": "tsc --project tsconfig.json --pretty --noEmit",
    "gql": "graphql-codegen --config codegen.js -r dotenv/config",
    "gql:watch": "yarn gql --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.5.4",
    "@auth0/auth0-react": "^1.8.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fluentui/react": "^8.48.1",
    "@fluentui/react-hooks": "^8.3.8",
    "@fluentui/react-icons": "^1.1.145",
    "@fluentui/theme-samples": "^8.2.60",
    "@fluentui/web-components": "^2.1.0",
    "dayjs": "^1.10.7",
    "modern-normalize": "^1.1.0",
    "next": "^12.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-autocomplete": "^2.6.1",
    "react-hook-form": "^7.22.5"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.7.2",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@types/node": "17.0.8",
    "@types/react": "^17.0.36",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "gql": "^1.1.2",
    "graphql": "^16.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "typescript": "4.5.4"
  }
}
