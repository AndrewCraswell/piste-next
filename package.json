{
  "name": "fencing-club",
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "preview": "vite preview",
    "dev": "yarn dev:web",
    "dev:api": "yarn build:api && swa start http://localhost:3000 --api-location ./api --run \"yarn dev:api && yarn dev:web\"",
    "dev:web": "vite",
    "dev:build": "yarn build && swa start ./dist --api-location ./api",
    "build": "yarn build:web && yarn build:api",
    "build:api": "cd api && yarn build",
    "build:web": "yarn lint:ts && vite build",
    "lint": "eslint src",
    "lint:ts": "tsc --project tsconfig.json --pretty --noEmit",
    "gql": "cross-env DOTENV_CONFIG_PATH=.env.local graphql-codegen --config codegen.js -r dotenv/config",
    "gql:watch": "yarn gql --watch",
    "prepare": "husky install",
    "postinstall": "cd api && yarn install"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@auth0/auth0-react": "^1.10.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fluentui/react": "^8.72.1",
    "@fluentui/react-components": "^9.0.0-rc.14",
    "@fluentui/react-hooks": "^8.5.5",
    "@fluentui/react-icons": "^2.0.166-rc.3",
    "@fluentui/react-make-styles": "^9.0.0-beta.4",
    "@fluentui/theme-samples": "^8.4.41",
    "@loadable/component": "^5.15.2",
    "@nylas/components-availability": "^1.1.4",
    "@nylas/components-schedule-editor": "^1.1.4-canary.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.31.0",
    "dayjs": "^1.11.2",
    "modern-normalize": "^1.1.0",
    "nylas": "^6.4.0",
    "polished": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-autocomplete": "^2.6.1",
    "react-hook-form": "^7.31.3",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "stripe": "^9.6.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.0.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.9",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.14",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "17.0.40",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@vitejs/plugin-react": "^1.3.0",
    "azure-functions-core-tools": "^4.0.4544",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "gql": "^1.1.2",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.9"
  }
}
